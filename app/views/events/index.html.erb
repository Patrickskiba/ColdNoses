<%= stylesheet_link_tag 'nonHomeNav', media: 'print, screen' %>

<p id="notice"><%= notice %></p>

<div id="calendar"></div>
  <table>
      <thead>
      <tr>
          <th>Title</th>
         <th>Description</th>
         <th>Start time</th>
          <th>End time</th>
         <th colspan="3"></th>
        </tr>
    </thead>
    <tbody>
        <% @events.each do |event| %>
              <tr>
             <td><%= event.title %></td>
              <td><%= event.description %></td>
              <td><%= event.start_time %></td>
              <td><%= event.end_time %></td>
            </tr>
            <% end %>
      </tbody>
    </table>
  <br/>
  <% if current_user.try(:admin?) %>
  <%= link_to 'New Event', new_event_path, class:"btn btn-success" %>
<%end%>


<br/>